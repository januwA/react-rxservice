{
  "name": "ajanuw-react-rxservice",
  "version": "1.7.0",
  "description": "react service is a react state manager",
  "private": false,
  "main": "./dist/umd/react-rxservice.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development ts-node config/webpack/dev-server.ts",
    "esm": "tsc --project tsconfig.esm.json",
    "umd": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production webpack -c config/webpack/prod.config.ts",
    "build": "npm run umd && npm run esm",
    "test": "ts-node  --project tsconfig.test.json test.ts"
  },
  "keywords": [
    "react-rxservice",
    "react",
    "rxjs",
    "rx",
    "react rxjs",
    "react service",
    "useService",
    "RxService",
    "Ajanuw"
  ],
  "author": "Ajanuw",
  "license": "MIT",
  "homepage": "https://github.com/januwA/react-rxservice",
  "repository": {
    "type": "git",
    "url": "https://github.com/januwA/react-rxservice.git"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/webpack": "^5.28.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.5.0",
    "json5": "^2.2.0",
    "puppeteer": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
